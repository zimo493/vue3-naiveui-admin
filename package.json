{
  "name": "vue3-naiveui-admin",
  "private": true,
  "version": "2.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit & vite build",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint:eslint": "eslint --cache \"{src,build}/**/*.{vue,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write \"**/*.{js,cjs,ts,tsx,json,css,scss,vue,html,md}\"",
    "lint:stylelint": "stylelint --cache \"**/*.{css,scss,vue}\" --fix",
    "lint:lint-staged": "lint-staged",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky",
    "commit": "git-cz"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{cjs,json}": [
      "prettier --write"
    ],
    "*.{vue,html}": [
      "eslint --fix",
      "prettier --write",
      "stylelint --fix"
    ],
    "*.{scss,css}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@stomp/stompjs": "^7.2.1",
    "@vueuse/core": "^14.1.0",
    "@wangeditor-next/editor": "^5.6.49",
    "@wangeditor-next/editor-for-vue": "^5.1.14",
    "axios": "^1.13.2",
    "colord": "^2.9.3",
    "echarts": "^6.0.0",
    "highlight.js": "^11.11.1",
    "jsencrypt": "^3.5.4",
    "pinia": "^3.0.4",
    "pinia-plugin-persistedstate": "^4.7.1",
    "qs": "^6.14.0",
    "radash": "^12.1.1",
    "vue": "^3.5.25",
    "vue-cropper": "^1.1.4",
    "vue-i18n": "^11.2.2",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@commitlint/types": "^20.2.0",
    "@eslint/js": "^9.39.2",
    "@iconify-json/icon-park-outline": "^1.2.4",
    "@iconify/vue": "^5.0.0",
    "@types/node": "^25.0.3",
    "@types/qs": "^6.14.0",
    "@unocss/vite": "^66.5.10",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitejs/plugin-vue-jsx": "^5.1.2",
    "@vue/tsconfig": "^0.8.1",
    "commitizen": "^4.3.1",
    "cz-git": "^1.12.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "^10.6.2",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "naive-ui": "^2.43.2",
    "postcss": "^8.5.6",
    "postcss-html": "^1.8.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.7.4",
    "rollup": "^4.53.5",
    "sass": "^1.97.0",
    "stylelint": "^16.26.1",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recess-order": "^7.4.0",
    "stylelint-config-recommended-scss": "^16.0.2",
    "stylelint-config-recommended-vue": "^1.6.1",
    "stylelint-config-standard": "^39.0.1",
    "stylelint-prettier": "^5.0.3",
    "terser": "^5.44.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.0",
    "unocss": "^66.5.10",
    "unplugin-auto-import": "^20.3.0",
    "unplugin-icons": "^22.5.0",
    "unplugin-vue-components": "^30.0.0",
    "vite": "^7.3.0",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.1.8"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
