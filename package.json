{
  "name": "vue3-naiveui-admin",
  "private": true,
  "version": "1.3.6",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit & vite build",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint:eslint": "eslint --cache \"src/**/*.{vue,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write \"**/*.{js,cjs,ts,tsx,json,css,scss,vue,html,md}\"",
    "lint:stylelint": "stylelint --cache \"**/*.{css,scss,vue}\" --fix",
    "lint:lint-staged": "lint-staged",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky",
    "commit": "git-cz"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{cjs,json}": [
      "prettier --write"
    ],
    "*.{vue,html}": [
      "eslint --fix",
      "prettier --write",
      "stylelint --fix"
    ],
    "*.{scss,css}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@stomp/stompjs": "^7.1.1",
    "@vueuse/core": "^13.5.0",
    "@wangeditor-next/editor": "^5.6.38",
    "@wangeditor-next/editor-for-vue": "^5.1.14",
    "axios": "^1.10.0",
    "colord": "^2.9.3",
    "echarts": "^5.6.0",
    "highlight.js": "^11.11.1",
    "js-cookie": "^3.0.5",
    "jsencrypt": "^3.3.2",
    "pinia": "^3.0.3",
    "pinia-plugin-persistedstate": "^4.4.1",
    "qs": "^6.14.0",
    "radash": "^12.1.1",
    "vue": "^3.5.17",
    "vue-cropper": "^1.1.4",
    "vue-i18n": "^11.1.10",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@commitlint/types": "^19.8.1",
    "@eslint/js": "^9.31.0",
    "@iconify-json/icon-park-outline": "^1.2.2",
    "@iconify/vue": "^4.3.0",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^22.16.5",
    "@types/qs": "^6.14.0",
    "@unocss/vite": "^66.3.3",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitejs/plugin-vue-jsx": "^4.2.0",
    "@vue/tsconfig": "^0.7.0",
    "commitizen": "^4.3.1",
    "cz-git": "^1.12.0",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-vue": "^10.3.0",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.2",
    "naive-ui": "^2.42.0",
    "postcss": "^8.5.6",
    "postcss-html": "^1.8.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.6.2",
    "rollup": "^4.45.1",
    "sass": "^1.89.2",
    "stylelint": "^16.22.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recess-order": "^6.1.0",
    "stylelint-config-recommended-scss": "^14.1.0",
    "stylelint-config-recommended-vue": "^1.6.1",
    "stylelint-config-standard": "^38.0.0",
    "stylelint-prettier": "^5.0.3",
    "terser": "^5.43.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.37.0",
    "unocss": "^66.3.3",
    "unplugin-auto-import": "^19.3.0",
    "unplugin-icons": "^22.1.0",
    "unplugin-vue-components": "^28.8.0",
    "vite": "^6.3.5",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^2.2.12"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
